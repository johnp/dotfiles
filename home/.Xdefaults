! urxvt configuration
!! .Xresources isn't sourced on wayland, but .Xdefaults is sourced by urxvt
URxvt*buffered: true
URxvt*cursorBlink: true
URxvt*underlineColor: yellow
!!Urxvt*font: xft:Inconsolata\ for\ Powerline:pixelsize=10:antialias=true
!!URxvt*font: xft:Sauce\ Code\ Powerline:pixelsize=13:antialias=true:hinting=true
!!URxvt*font: xft:Fura\ Mono\ for\ Powerline\ Nerd\ Font\ Complete Mono:pixelsize=13:antialias=true:hinting=true
!!URxvt*font: xft:Fura\ Mono\ for\ Powerline\ Nerd\ Font\ Complete Mono:pixelsize=13:antialias=true:hinting=true
URxvt*font: xft:Fura\ Mono\ for\ Powerline:pixelsize=13:antialias=true:hinting=true, \
            xft:Noto\ Sans\ Symbols, \
            xft:Noto\ Sans\ Emoji
URxvt*depth: 32
URxvt*borderless: true
URxvt*scrollBar: false
URxvt*loginShell: true
Urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
URxvt*saveLines: 5000
URxvt*termName: rxvt-unicode
URxvt.perl-ext-common: default,matcher,selection-to-clipboard
URxvt.urlLauncher: firefox-nightly
URxvt.matcher.button: 1

!! color theme
!!#include ".Xresources.d/gruvbox-urxvt256.xresources"
#include ".Xresources.d/solarized-urxvt.xresources"
!!#include ".Xresources.d/tango-urxvt.xresources"
